# Ignore directories generated by Composer
/drush/contrib/
/drush/Commands/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/*
!/web/libraries/chosen
/console/

# Ignore Scaffold files.
.drush-lock-update
.gitattributes
web/.eslintrc.json
web/INSTALL.txt
web/README.txt
web/example.gitignore
web/.csslintrc
web/.editorconfig
web/.eslintignore
web/.eslintrc
web/.htaccess
web/.ht.router.php
web/autoload.php
web/index.php
web/robots.txt
web/sites/default/default.services.yml
web/sites/default/default.settings.php
web/sites/development.services.yml
web/sites/example.settings.local.php
web/sites/default/settings.local.php
web/sites/example.sites.php
web/sites/README.txt
web/update.php
web/web.config

# Ignore Drupal's file directory
/web/sites/default/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Exclude platform.sh build files.
/.platform/local
/_www

# Ignore files generated by editors
/.idea/
.vscode

# Ignore Lando local files
.lando.local.yml

# Ignore .env files as they are personal
/.env
/.env.local

# Lint cache
.phplint-cache

# Ignored compiled assets
/node_modules/
/web/themes/custom/**/dist/

# Ignore files only needed on CI
.bashrc

